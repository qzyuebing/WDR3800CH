
config init
	option stdout '1'
	option stderr '1'
	option respawn '1'
	option user 'root'
	option group 'root'

config conf 'common'
	option tls_enable 'false'
	option login_fail_exit 'false'
	option token '12345'
	option server_port '7600'
	option server_addr 'oscam.picp.net'

config conf 'Unicom_UDPX'
	option type 'tcp'
	option use_encryption 'false'
	option use_compression 'false'
	option local_ip '192.168.10.1'
	option local_port '4066'
	option remote_port '4066'

config conf 'Unicom_WEB'
	option type 'tcp'
	option use_encryption 'false'
	option use_compression 'false'
	option local_ip '192.168.10.1'
	option local_port '80'
	option remote_port '4067'

config conf 'Unicon_SSH'
